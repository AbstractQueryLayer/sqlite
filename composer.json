{
  "name": "ifcastle/aql-sqlite",
  "description": "SQLite storage adapter for AQL",
  "keywords": [
    "aql",
    "query",
    "language",
    "storage"
  ],
  "type": "ifcastle-aql-package",
  "license": "MIT",
  "authors": [
    {
      "name": "Edmond Dantes",
      "email": "edmondifthen@proton.me"
    }
  ],
  "require": {
    "php": ">=8.4",
    "ifcastle/aql-dsl": "^0.1.0",
    "ifcastle/aql-executor": "^0.1.0",
    "ifcastle/aql-generator": "^0.1.0",
    "ifcastle/aql-pdodriver": "^0.1.0",
    "ifcastle/aql-storage": "^0.1.0",
    "ext-pdo": "*",
    "ifcastle/aql-entity": "^0.1.0",
    "ifcastle/aql-sqldriver": "^0.1.0"
  },
  "autoload": {
    "psr-4": {
      "IfCastle\\AQL\\SQLite\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IfCastle\\AQL\\SQLite\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^11.2",
    "rector/rector": "^1.2",
    "ifcastle/codestyle": "^0.2.0"
  },
  "scripts": {
    "test": "vendor/bin/phpunit ./tests",
    "cs-fix": "php-cs-fixer fix",
    "cs-check": "php-cs-fixer fix --dry-run --diff",
    "rector": "rector process",
    "rector-dry": "rector process --dry-run"
  }
}
